@import 'general.css';

#content {
    position: relative;
    width: 960px;
    margin-top: 20px;
    margin-bottom: 20px;
}

/**
 * *******************
 *  HEADER AND MENU
 * *******************
**/

#header {
    margin-bottom: 25px;
}

#header h1 {
    float: left;
    margin: 0;
    padding: 0;
    padding-left: 5px;
    color: #666;
    font-size: 24px;
    text-shadow: #fff 1px 1px;
}

#header ul#right_sub {
    list-style: none;
    float: right;
    margin: 0;
    padding: 0;
}

#header ul#right_sub li {
  float: left;
  display: inline;
  margin-left: 10px;
  padding: 3px;
  line-height: 24px;
}

#header ul#right_sub li a {
    text-decoration: none;
    padding: 4px;
}

#header ul#right_sub li a img {
    text-decoration: none;
    border: 0;
}

#header ul#right_sub li a span {
    color: #555;
    font-size: 12px;
    vertical-align: 3px;
    padding-left: 5px;
}

#main_sections {
    background-color: #323e69;
    width: 100%;
    /*height: 30px;*/
    line-height: 40px;
    /*margin-bottom: 15px;*/
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
    /*-moz-border-radius-topleft: 5px;
    -moz-border-radius-topright: 5px;
    -moz-border-radius-bottomright: 5px;
    -webkit-border-top-left-radius: 5px;
    -webkit-border-top-right-radius: 5px;
    -webkit-border-bottom-right-radius: 5px;*/
}

#main_sections ul {
    list-style: none;
    float: right;
    margin: 0;
    padding: 0;
    padding-right: 10px;
    /*line-height: 35px;*/
    font-size: 12px;
}

#main_sections ul li {
  float: left;
  display: inline;
}

#main_sections ul li a {
    text-decoration: none;
}

#main_sections ul li span {
    color: #ffffff;
    text-shadow: #07133d 1px 1px;
    padding-left: 10px;
}

#main_sections ul li a.important_link span {
    color: #bbb;
}

#main_sections ul#left_sec_main {
  float: left;
  display: inline;
  margin-left: 5px;
  margin-right: 10px;
}

#main_sections ul#left_sec_main li a {
    text-decoration: none;
    padding: 4px;
	color: #fff;
    font-size: 12px;
    margin-right: 10px;
}

#main_sections ul#left_sec_main li.selected a {
    color: #fff;
    font-weight: bold;
    text-shadow: #07133d 1px 1px;
}

#main_sections ul#left_sec_main li.inactive a {
    /*background: url(../img/warning_icon.png) no-repeat center left;*/
    color: #777;
    font-size: 12px;
	cursor: text;
    /*padding-left: 18px;*/
    /*text-decoration: line-through;*/
}

#sub_sections {
    width: 100%;
    line-height: normal;
}

/**
 * END HEADER AND MENU
**/


#page_content {
    width: 100%;
}

.left_center_container {
    width: 75%;
    float: left;
}

#left_section {
   float: left;
   width: 27%;
   padding-top: 35px;
}

#left_section ul {
    width: 100%;
    list-style: none;
    float: left;
    margin: 0;
    padding: 0;
    line-height: normal;
}

#left_section ul li {
    padding: 7px;
    -moz-border-radius-topleft: 8px;
    -moz-border-radius-bottomleft: 8px;
    -webkit-border-top-left-radius: 8px;
    -webkit-border-bottom-left-radius: 8px;
}

#left_section ul li.section_active {
    background-color: #fff;
    -webkit-box-shadow: 0px 0px 10px rgb(204,204,204);
    -moz-box-shadow: 0 0 10px rgb(204,204,204);
	box-shadow: 0px 0px 10px rgb(204,204,204);
}

#left_section ul li a {
    padding-left: 5px;
    text-decoration: none;
}

#left_section ul li a img {
    border: 0;
    padding: 0;
    padding-right: 8px;
}

#left_section ul li a span {
    color: #777;
    font-size: 11px;
    vertical-align: 3px;
}

#left_section ul li.section_active a span {
    color: #444;
    font-weight: bold;
}

#left_section ul li.section_active:hover a span {
    text-decoration: none;
}

#left_section ul li:hover a span {
    text-decoration: underline;
}

#center_section {
    float: left;
    width: 73%;
    -moz-border-radius: 8px;
    -webkit-border-radius: 8px;
	border-radius: 8px;
    -webkit-box-shadow: 0px 0px 10px rgb(204,204,204);
    -moz-box-shadow: 0 0 10px rgb(204,204,204);
	box-shadow: 0px 0px 10px rgb(204,204,204);
    background-color: #ffffff;
}

