    font-size: 12px;
}

/* EDIT POPUP */
#edit_dialog form {
    margin: 0;
    padding: 0;
}

#edit_dialog form p.general_p {
    padding: 10px;
    margin: 0;
    font-size: 12px;
    font-weight: normal;
    text-align: left;
}

#edit_dialog form p span {
    font-weight: bold;
}

#edit_dialog table {
	padding: 5px 15px 5px 30px;
}

#edit_dialog form td.left,
#edit_dialog form td.right {
    padding-right: 10px;
    padding-bottom: 5px;
}

#edit_dialog form td.left{
	width: 70px;
}

#edit_dialog form td.right{
	min-width: 70px;
}

#edit_dialog form td.right img {
    display: inline-block;
    margin: 0;
    padding: 0;
	/*padding: 5px;*/
    padding-top: 5px;
    padding-left: 5px;
    border: 0;
}

#edit_dialog form td.form_error{
	min-width: 70px;
}


#edit_dialog form input.input,
#edit_dialog form select {
    /*background: #ffffff url(../img/shadow_input.png) top left repeat-x;*/
    background-color: #ffffff;
    border: 1px solid #ccc;
    height: 26px;
    width: 170px;
	float:left;
    font-size: 11px;
}

#edit_dialog form input.btn,
#edit_dialog form input.btn_del{
    /*margin: 15px 0;*/
    margin-bottom: 10px;
    padding: 5px;
    float: right;
	clear:both;
    color: #fff;
    font-weight: bold;
    font-size: 11px;
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
	border-radius: 5px;
	cursor: pointer;
}

#edit_dialog form input.btn{
	background-color: #6875a6;
    border: 1px solid #323e69;
	text-shadow: #323e69 1px 1px;
}
#edit_dialog form input.btn:hover {
	background-color: #323e69;
}

#edit_dialog form input.btn_del{
	background-color: #cf6767;
    border: 1px solid #af4141;
	text-shadow: #7F0F29 1px 1px;
}

#edit_dialog form input.btn_del:hover {
	background-color: #af4141;
}

#edit_dialog form label {
    color: #000;
    font-size: 11px;
    font-weight: normal;
}

#edit_dialog form td.form_error span.error {
    font-size: 11px;
    color: red;
    /*padding-left: 10px;*/
    line-height: 25px;
}